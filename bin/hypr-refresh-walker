#!/bin/bash

# Ensure walker is set to autostart
mkdir -p ~/.config/autostart/
cp /home/cactus/my_omarchy/installation_hyprland/autostart/walker.desktop ~/.config/autostart/

hypr-refresh-config walker/config.toml
hypr-refresh-config elephant/calc.toml
hypr-refresh-config elephant/desktopapplications.toml
hypr-restart-walker